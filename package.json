{
  "name": "avi-bun",
  "module": "index.ts",
  "license": "UNLICENSED",
  "scripts": {
    "start": "bun scripts/with.remote.env.t bun ./src/index.ts",
    "dev": "bun scripts/with.remote.env.ts bun --watch --inspect=ws://localhost:6499/dev ./src/index.ts",
    "with:env": "bun scripts/with.remote.env.ts",
    "docker:dev:tail": "docker logs --follow avi-bun-server-1",
    "codegen": "prisma generate",
    "build": "bun build ./src/index.ts --compile --outfile .app",
    "lint": "eslint . --ext .ts",
    "lint:fix": "bun lint -- --fix",
    "test": "NODE_ENV=test bun test --bail",
    "test:coverage": "NODE_ENV=test bun test --coverage",
    "postinstall": "bunx patch-package",
    "prisma:generate": "bunx prisma generate",
    "prisma:deploy": "bunx prisma migrate deploy",
    "prisma:dev": "bunx prisma migrate dev --create-only",
    "prisma:seed": "bunx prisma db seed",
    "ext:services": "sh ./scripts/run.services.sh",
    "ext:services:down": "sh ./scripts/down.services.sh"
  },
  "prisma": {
    "seed": "bun scripts/with.remote.env.ts bun ./prisma/seed/index.ts"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/object-hash": "^3.0.6",
    "@types/request-ip": "^0.0.41",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "bun-types": "latest",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.2",
    "prisma": "^5.8.1",
    "prisma-typegraphql-types-generator": "^1.5.14",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "class-validator": "^0.14.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@prisma/client": "^5.8.1",
    "@sentry/bun": "^7.93.0",
    "cheerio": "1.0.0-rc.12",
    "cron-parser": "^4.9.0",
    "cron-time-generator": "^2.0.3",
    "elysia": "^0.8.9",
    "firebase-admin": "^12.0.0",
    "generate-unique-id": "^2.0.3",
    "graphql-scalars": "^1.22.4",
    "html-to-md": "^0.8.5",
    "ky": "^1.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.44",
    "object-hash": "^3.0.0",
    "patch-package": "^8.0.0",
    "reflect-metadata": "^0.2.1",
    "request-ip": "^3.3.0",
    "short-unique-id": "^5.0.3",
    "try-nice": "^4.0.4",
    "tslog": "^4.9.2",
    "type-graphql": "2.0.0-beta.3",
    "uuid": "^9.0.1",
    "yup": "^1.3.3"
  }
}
