{
  "name": "avi-bun",
  "module": "index.ts",
  "license": "UNLICENSED",
  "scripts": {
    "start": "bun ./src/index.ts",
    "dev": "bun --watch --inspect ./src/index.ts",
    "codegen": "prisma generate",
    "build": "bun build ./src/index.ts --compile --outfile .app",
    "lint": "eslint . --ext .ts",
    "lint:fix": "bun lint -- --fix",
    "test": "NODE_ENV=test bun test --bail",
    "test:coverage": "NODE_ENV=test bun test --coverage",
    "postinstall": "bunx patch-package",
    "prisma:generate": "bunx prisma generate",
    "prisma:deploy": "bunx prisma migrate deploy",
    "prisma:dev": "bunx prisma migrate dev --create-only",
    "prisma:seed": "bunx prisma db seed",
    "prisma:reset": "bun ./prisma/reset.ts",
    "ext:services": "sh ./scripts/run.services.sh",
    "ext:services:down": "sh ./scripts/down.services.sh"
  },
  "prisma": {
    "seed": "bun ./prisma/seed/index.ts"
  },
  "devDependencies": {
    "@prisma/generator-helper": "^5.5.2",
    "@types/lodash": "^4.14.200",
    "@types/request-ip": "^0.0.40",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "bun-types": "latest",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-perfectionist": "^2.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "prisma-typegraphql-types-generator": "^1.5.14",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "typescript": "^5.2.2",
    "class-validator": "^0.14.0"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@prisma/client": "5.5.2",
    "@sentry/bun": "^7.77.0",
    "cheerio": "1.0.0-rc.12",
    "cron-parser": "^4.9.0",
    "cron-time-generator": "^2.0.1",
    "elysia": "^0.7.21",
    "firebase-admin": "^11.11.0",
    "generate-unique-id": "^2.0.3",
    "gitops-secrets": "^0.0.6",
    "graphql-scalars": "^1.22.4",
    "ky": "^1.1.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "prisma": "5.5.2",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "try-nice": "^4.0.4",
    "tslog": "^4.9.2",
    "type-graphql": "2.0.0-beta.3",
    "uuid": "^9.0.1",
    "yup": "^1.3.2"
  }
}
